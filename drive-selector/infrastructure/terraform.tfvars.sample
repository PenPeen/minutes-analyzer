# Environment configuration
environment = "development"

# AWS configuration
aws_region = "ap-northeast-1"

# API Gateway configuration (optional)
api_gateway_domain = ""

# Lambda configuration
process_lambda_arn = "arn:aws:lambda:ap-northeast-1:123456789012:function:minutes-analyzer-development"

