# Non-sensitive configuration values
# This file can be safely committed to version control

# AWS Region
aws_region = "ap-northeast-1"

# Environment
environment = "local"

# Project name
project_name = "minutes-analyzer"

# Lambda configuration
lambda_timeout     = 30
lambda_memory_size = 128

# Log retention
log_retention_days = 14

# Secrets configuration
gemini_api_key_secret_name = "minutes-analyzer/gemini-api-key"


# Log level
log_level = "INFO"

# AI Model
ai_model = "gemini-2.5-flash"

# NOTE: Sensitive variables should be set in .env.tfvars
# See .env.tfvars.sample for required sensitive variables